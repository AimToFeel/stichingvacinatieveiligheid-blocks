		"warning": {
			"type": "string",
			"source": "html",
			"selector": "#attribute-warning"
		},
		"warningHidden": {
			"type": "boolean",
			"source": "attribute",
			"selector": "#attribute-warning",
			"attribute": "hidden",
			"default": true
		}
	},
	"example": {
		"attributes": {
			"content": "Dit is een accordion.",
			"title": "Titel"
		}
	},
    "editorScript": "file:./block.js"
